let rec plus = \x.\y. 
    if x=0 then y 
    else S (plus (minus_one x) y) 
in 

let rec minus = \x. \y. 
    if y=0 then x 
    else minus (minus_one x) (minus_one y) 
in

let rec fib = \n. 
    if n=0 then 0 
    else if n = S 0 then S 0 
    else plus (fib (minus_one n)) (fib (minus_one (minus_one n))) 
in
 
